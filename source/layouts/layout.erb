<!doctype html>
<html>
  <head>
      <meta content="IE=edge" http-equiv="X-UA-Compatible">
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
      <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,800,700">
      <!-- Use title if it's in the page YAML frontmatter -->
      <title>
          <%= current_page.data.title || "Junior Comtec e.V. Darmstadt" %>
      </title>
      <link href='//fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>
      <%= stylesheet_link_tag :site %>

  </head>


  <body class="<%= page_classes %>">
      <%= yield %>

      <%= javascript_include_tag 'vendor/jquery' %>
      <%= javascript_include_tag 'vendor/bootstrap-sass/bootstrap.min' %>
      <%= javascript_include_tag :all %>
  </body>

</html>
